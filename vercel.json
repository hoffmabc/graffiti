{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ],
    "env": {
      "VITE_RPC_URL": "@vite_rpc_url",
      "VITE_PROGRAM_PUBKEY": "@vite_program_pubkey",
      "VITE_WALL_ACCOUNT_PUBKEY": "@vite_wall_account_pubkey"
    }
  }